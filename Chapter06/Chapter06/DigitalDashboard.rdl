  <DataSets>
    <DataSet Name="PickupsAndLost">
      <Query>
        <DataSourceName>Galactic</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@GaugeDate">
            <Value>=Parameters!GaugeDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        COUNT(Delivery.DeliveryNumber) AS NumOfPickups, COUNT(LostPackage.LostPackageNumber) AS NumLost
FROM            Delivery LEFT OUTER JOIN
                         LostPackage ON Delivery.DeliveryNumber = LostPackage.DeliveryNumber
WHERE        (Delivery.PickupDateTime BETWEEN DATEADD(d, - 28, @GaugeDate) AND @GaugeDate)</CommandText>
      </Query>
      <Fields>
        <Field Name="NumOfPickups">
          <DataField>NumOfPickups</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="NumLost">
          <DataField>NumLost</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
