  <DataSources>
    <DataSource Name="Galactic">
      <DataSourceReference>Galactic</DataSourceReference>
      <rd:SecurityType>None</rd:SecurityType>
      <rd:DataSourceID>279e4b9e-0ac8-41bd-86b5-880dfc0a461d</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="FuelPrices">
      <Query>
        <DataSourceName>Galactic</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Year">
            <Value>=Parameters!Year.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@PropulsionType">
            <Value>=Parameters!PropulsionType.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        Propulsion.Description AS FuelType, FuelPrice.PriceStartDate, FuelPrice.Price
FROM            FuelPrice INNER JOIN
                         Propulsion ON FuelPrice.PropulsionID = Propulsion.PropulsionID
WHERE        (YEAR(FuelPrice.PriceStartDate) = @Year) AND (Propulsion.Description = @PropulsionType) OR
                         (YEAR(FuelPrice.PriceStartDate) = @Year) AND (@PropulsionType = 'All')
ORDER BY FuelType, FuelPrice.PriceStartDate</CommandText>
      </Query>
      <Fields>
        <Field Name="FuelType">
          <DataField>FuelType</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PriceStartDate">
          <DataField>PriceStartDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Price">
          <DataField>Price</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Years">
      <Query>
        <DataSourceName>Galactic</DataSourceName>
        <CommandText>SELECT DISTINCT YEAR(PriceStartDate) AS year
FROM            FuelPrice
ORDER BY year</CommandText>
      </Query>
      <Fields>
        <Field Name="year">
          <DataField>year</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="FuelTypes">
      <Query>
        <DataSourceName>Galactic</DataSourceName>
        <CommandText>SELECT        'All' AS FuelType, '_All' AS SortField
UNION
SELECT        Description, Description AS SortField
FROM            Propulsion
ORDER BY SortField</CommandText>
      </Query>
      <Fields>
        <Field Name="FuelType">
          <DataField>FuelType</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SortField">
          <DataField>SortField</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
